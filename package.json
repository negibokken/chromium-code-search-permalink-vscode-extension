{
  "name": "chromium-code-search-permalink",
  "displayName": "chromium-code-search-permalink",
    "repository": "https://github.com/negibokken/chromium-code-search-permalink-vscode-extension",
  "description": "",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
        "onCommand:chromium-code-search-permalink.copy-permalink"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "chromium-code-search-permalink.copy-permalink",
        "title": "Copy Chromium Code Search Permalink"
      }
    ]
  },
  "publisher": "negibokken",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5",
    "ts-loader": "^9.2.8",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "@vscode/test-electron": "^2.1.3"
  }
}
